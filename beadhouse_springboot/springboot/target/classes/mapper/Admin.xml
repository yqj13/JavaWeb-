<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace= "com.example.springboot.mapper.AdminMapper">


    <select id="selectByUsername" resultType="com.example.springboot.entity.Admin">
        select distinct userName, password from admin where userName = #{userName}
    </select>
</mapper>